{% extends "layouts/default.twig" %}

{% block content %}

	<form action="{{ getBaseUrl('/register') }}" method='post'>

		<div class="input">
			<input type="email" name="email">
		</div>

		<div class="input">
			<input type="password" name="password">
		</div>

		<button type="submit">Login</button>

	</form>

{% endblock %}
